[flake8]
max-line-length = 88
ignore = D100,D104,W503
extend-ignore = E203
exclude = .git,.mypy_cache,.pytest_cache,__pycache__,.venv,xenapiservice/vendors/XenAPI.py,test_xenservice.py
extend-immutable-calls = Body, fastapi.Body, fastapi.params.Body, Depends, fastapi.Depends, fastapi.params.Depends
per-file-ignores =
    tests/*:S101,S106
